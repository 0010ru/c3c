module test;
import std::io;
struct Abc
{
	float m00, m01, m10, m11;
}

Abc a = flip(Abc { 1, 2, 3, 4 });

macro flip($init)
{
	return Abc { $init.m01, $init.m10, $init.m11, $init.m00 };
}

macro check_type($Type)
{
	var $b = $Type[4][4] {};
	$Type z = $b[2][2];
	return z;
}

enum Blurb { FOO }

define Bdd = distinct Abc;
fn void main()
{
	var $i = int[4] { 1, 2, 3, 4 };
	var $b = Abc[2] { Abc {}, Abc { 11, 22, 33, 44 }};
	var $c = variant[4] {};
	check_type(int);
	check_type(Abc);
	check_type(anyerr);
	check_type(Blurb);
	check_type(int*);
	check_type(bool);
	check_type(typeid);
	check_type(int[<3>]);
	check_type(Bdd);
	check_type(int[2]);
	check_type(int[]);
	io::printfln("%d", $b[0].m11);
}

